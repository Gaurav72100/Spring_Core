<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        
        
        
   <bean id="add" class="com.entity.Address" scope="prototype">
   <property name="city" value="Noida"/>
   <property name="state" value="Uttar Pradesh"/>
   </bean>
   
   <bean id="getAdd" class="com.entity.PermanentAddress">
   <lookup-method name="getAdde" bean="add"/>
   </bean>
        
        
 </beans>