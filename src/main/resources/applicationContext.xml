<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
         http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

        <context:annotation-config/>
        
        
    <bean name="add" class="com.entity.Address">
    <property name="city" value="pink city"></property>
    <property name="state" value="Rajasthan"></property>
    </bean>   
    
     <bean name="add2" class="com.entity.Address">
    <property name="city" value="pink city2"></property>
    <property name="state" value="Rajasthan"></property>
    </bean>    
        
     <bean name="emp" class="com.entity.Employee">
     <property name="id" value="108"></property>
     <property name="name" value="Gopal Krishna"/>
     <property name="salary" value="5000000"/>
     
     
     
     
     
     </bean>   
        
 </beans>